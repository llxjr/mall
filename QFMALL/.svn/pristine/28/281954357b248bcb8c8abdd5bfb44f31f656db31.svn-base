.video_container .course_state.course_learning .course_state_icon{background-position:-450px -150px}
.video_container .video_wrapper{padding-top:10px}
.video_container .video_box{margin:0 auto;width:1143px;height:656px;background:#6d72aa}
.video_container .video_bottom{padding-top:10px;margin:0 auto;width:1143px; line-height:20px;font-size: 12px;
    color:#7e878c;}
.video_container .video_bottom .play_btn{
    display: inline-block;
    margin-left: 20px;
    width: 14px;
    height: 14px;
    background: url("../images/icon_all.png") no-repeat -250px -150px;
    cursor: pointer;
}
.video_container .video_bottom .video_time{
    margin-left: 20px;
}
.video_container .video_bottom_right{
    white-space: nowrap;
    height: 20px;
    line-height: 20px;
}
.video_container .video_bottom .ratio{
    display: inline-block;
    margin-right: 25px;
    vertical-align: middle;
}
.video_container .video_bottom .ratio_set{
    display: inline-block;
    vertical-align: middle;
    position: relative;
    margin-right: 25px;
    width: 20px;
    height: 20px;
}
.video_container .video_bottom .ratio_set:hover .ratio_seting_wrapper{
    display: block;
}
.video_container .video_bottom .setting_btn{
    display: inline-block;
    width: 20px;
    height: 20px;
    background: url("../images/icon_all.png") no-repeat -150px -200px;
    cursor: pointer;
}
.video_container .video_bottom .setting_btn:hover{
    background-position: -200px -200px;
}
.video_container .video_bottom .ratio_seting_title{
    margin: 20px 0;
}
.video_container .video_bottom .ratio_seting_wrapper{
    display: none;
    position: absolute;
    bottom: 20px;
    left: 50px;
    margin-left: -120px;
    padding:0 5px;
    width: 150px;
    height: 140px;
    background: rgba(0,0,0,.5);
    text-align: center;
    white-space: normal;

}
.video_container .video_bottom .ratio_seting_val_wrapper{
font-size: 0;
}
.video_container .video_bottom .ratio_seting_val{
    font-size: 12px;
    display: inline-block;
    width: 42px;
    height: 20px;
    margin :0 2px  9px;
    background: #1d1c24;
    color:#fff;
    cursor: pointer;
}
.video_container .video_bottom .ratio_seting_val.active,
.video_container .video_bottom .ratio_seting_val:hover{
    background: #6dbb2a;
}
.video_container .video_bottom .volume_wrapper{
    display: inline-block;
    margin-right: 34px;
}
.video_container .video_bottom .volume_icon{
    display: inline-block;
    vertical-align: middle;
    width: 20px;
    height: 20px;
    background: url("../images/icon_all.png") no-repeat -300px -198px;
}
.video_container .video_bottom .volume_value_bg{
    position: relative;
    display: inline-block;
    vertical-align: middle;
    margin-top: -2px;
    width: 70px;
    height: 2px;
    background:#7e878c;
}
.video_container .video_bottom .volume_value{
    position: absolute;
}
.video_container .video_bottom .volume_btn{
    position:absolute;
}
.video_container .video_bottom .full_screen{
    display: inline-block;
    margin-right: 20px;
    width: 20px;
    height:20px;
    background: url("../images/icon_all.png") no-repeat -250px -198px;
    cursor: pointer;
}
.video_recommend_container .category_title{margin-bottom:15px;color:#333;font-size:20px}
.video_recommend_container .category_title:before{display:none}
